<!-- Passenger Stairways -->

	<model>
	<name>Stairwayfront</name>
	<path>Aircraft/MD-10/Models/Operations/Stairways/stairs.xml</path>
		<condition>
		<equals>
			             <property>controls/switches/stairs1</property>
                                            <value type="bool">true</value>
		</equals>
		<less-than>
                <property>velocities/airspeed-kt</property>
                <value>10</value>
		</less-than>
		</condition>   
		<offsets>
			<x-m>-32.75</x-m>
			<y-m>-2.7</y-m>
			<z-m>-5.25</z-m>
			<heading-deg>270</heading-deg>
		</offsets>
	</model>

	<model>
	<name>Stairwayback</name>
	<path>Aircraft/MD-10/Models/Operations/Stairways/stairs.xml</path>
		<condition>
		<equals>
			             <property>controls/switches/stairs2</property>
                                            <value type="bool">true</value>
		</equals>
		<less-than>
                <property>velocities/airspeed-kt</property>
                <value>10</value>
		</less-than>
		</condition>   
		<offsets>
			<x-m> 24</x-m>
			<y-m>-2.7</y-m>
			<z-m>-5.25</z-m>
			<heading-deg>270</heading-deg>
		</offsets>
	</model>


	<model>
	<name>Fuel Truck</name>
	<path>Aircraft/MD-10/Models/Operations/Fuel/citerne.xml</path>
		<condition>
		<equals>
			             <property>controls/switches/fuel</property>
                                            <value type="bool">true</value>
		</equals>
		<less-than>
                <property>velocities/airspeed-kt</property>
                <value>15</value>
		</less-than>
		</condition>   
		<offsets>
			<x-m>-7</x-m>
            <y-m> 15</y-m>
            <z-m>-5.25</z-m>
		</offsets>
	</model>


	<model>
	<name>Bus</name>
	<path>Aircraft/MD-10/Models/Operations/Bus/bus.xml</path>
		<condition>
		<and>
			<property>controls/switches/bus</property>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>15</value>
            </less-than>
		</and>
		</condition>
		<offsets>
            <x-m>-20</x-m>
            <y-m>-15</y-m>
            <z-m>-5.25</z-m>
            <pitch-deg>0</pitch-deg>
            <heading-deg>-270</heading-deg>
        </offsets>
	</model>


	<model>
	<name>GPU truck long</name>
	<path>Aircraft/MD-10/Models/Operations/Baggage/chariot.xml</path>
		<condition>
		<and>
			<property>controls/switches/gpu</property>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>10</value>
            </less-than>
		</and>
		</condition>
		<offsets>
			<x-m>12</x-m>
            <y-m> 18</y-m>
            <z-m>-5.25</z-m>
            <pitch-deg>0</pitch-deg>
            <heading-deg>90</heading-deg>
        </offsets>
	</model>


