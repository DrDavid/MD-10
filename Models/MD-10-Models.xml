<!-- Airport Operations -->

	<model>
	<name>Followme</name>
	<path>Aircraft/MD-10/Models/Operations/Followme/followme.xml</path>
		<condition>
			<equals>
				<property>controls/switches/followme</property>
				<value type="bool">true</value>
			</equals>
			<less-than>
				<property>velocities/airspeed-kt</property>
				<value>80</value>
			</less-than>
		</condition>   
		<offsets>
			<x-m>-50</x-m>
			<y-m>0</y-m>
			<z-m>-5.25</z-m>
		</offsets>
	</model>

	<model>
	<name>Catering Vehicle</name>
	<path>Aircraft/MD-10/Models/Operations/Catering/catering.xml</path>
		<condition>
			<equals>
				<property>controls/switches/catering</property>
				<value type="bool">true</value>
			</equals>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>15</value>
            </less-than>
		</condition>   
		<offsets>
			<x-m>16</x-m>
			<y-m> 10</y-m>
			<z-m>-5.25</z-m>
			<heading-deg>180</heading-deg>
		</offsets>
	</model>

