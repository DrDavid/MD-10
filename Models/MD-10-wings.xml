			<entry><ind>0.3</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>45</dep></entry>
		</interpolation>
		<axis>
			<x1-m>-0.099</x1-m>
			<y1-m>-8.798</y1-m>
			<z1-m>4.662</z1-m>
			<x2-m>10.513</x2-m>
			<y2-m>-22.808</y2-m>
			<z2-m>5.456</z2-m>
		</axis>
	</animation>

