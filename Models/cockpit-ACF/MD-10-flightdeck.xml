<object-name>FlapLever</object-name>
<object-name>SlatLever</object-name>
<action>
<button>0</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>nasal</command>
<script>
controls.flapsDown(1);
</script>
</binding>
</action>
<action>
<button>1</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>nasal</command>
<script>
controls.flapsDown(-1);
</script>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>FlapLever</object-name>
<property>controls/flight/flaps</property>
<interpolation>
<entry><ind>0</ind><dep>-29</dep></entry>
<entry><ind>0.31</ind><dep>6</dep></entry>
<entry><ind>0.58</ind><dep>12</dep></entry>
<entry><ind>0.7</ind><dep>22</dep></entry>
<entry><ind>1</ind><dep>37</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>
<animation>
<type>rotate</type>
<object-name>SlatLever</object-name>
<property>controls/flight/slats</property>
<interpolation>
<entry><ind>0</ind><dep>-28</dep></entry>
<entry><ind>0.3</ind><dep>5</dep></entry>
<entry><ind>0.44</ind><dep>11</dep></entry>
<entry><ind>0.7</ind><dep>21</dep></entry>
<entry><ind>1</ind><dep>36</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>

<!-- Rembrandt !! -->
<effect>
<name>REMBRANDT transparency</name>
<inherits-from>Effects/model-transparent</inherits-from>
<object-name>TQMarkings</object-name>
</effect>

</PropertyList>