<!-- Speedbrake/spoiler lever -->
<animation>
<type>pick</type>
<object-name>SpeedbrakeLever</object-name>
<action>
<button>0</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-toggle</command>
<property>controls/flight/autospeedbrakes-armed</property>
<value>0</value>
<value>1</value>
</binding>
</action>
<action>
<button>4</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/speedbrake-lever</property>
<step>1</step>
<max>1</max>
</binding>
</action>
<action>
<button>3</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/speedbrake-lever</property>
<step>-1</step>
<min>0</min>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>SpeedbrakeLever</object-name>
<property>controls/flight/speedbrake-lever</property>
<interpolation>
<entry><ind>0</ind><dep>-33</dep></entry>
<entry><ind>1</ind><dep>2</dep></entry>
<entry><ind>2</ind><dep>10</dep></entry>
<entry><ind>3</ind><dep>18</dep></entry>
<entry><ind>4</ind><dep>40</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>
<animation>
<type>translate</type>
<object-name>SpeedbrakeLever</object-name>
<property>controls/flight/autospeedbrakes-armed</property>
<interpolation>
<entry><ind>0</ind><dep>-0.01</dep></entry>
<entry><ind>1</ind><dep>0</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>0</y>
<z>1</z>
</axis>
</animation>

