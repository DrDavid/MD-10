<!-- Pitch Trim Thing -->

    <animation>
        <type>pick</type>
        <object-name>ParkingBrake_0</object-name>
        <object-name>ParkingBrake_1</object-name>
        <action>
                <button>0</button>
                <repeatable>true</repeatable>
                <binding>
                        <command>property-adjust</command>
                        <property>/controls/flight/elevator-trim</property>
                        <step>-0.01</step>
                        <min>-0.8</min>
