		<object-name>No smoking</object-name>
		<condition>
			<or>
					<equals>
    	    	        <property>controls/cabin/NoSmoking-knob</property>
						<value>1</value>
					</equals>
					<equals>
    	    	        <property>controls/cabin/NoSmoking-knob</property>
						<value>0</value>
					</equals>
			</or>
		</condition>
	</animation>
	
	<animation>
