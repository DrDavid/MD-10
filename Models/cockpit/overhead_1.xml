		<object-name>oh.glass</object-name>
		<object-name>jett.cover</object-name>
	</animation>

    	<animation>
		<name>Panel lighting</name>
		<type>material</type>
		<object-name>overhead.text</object-name>
    		<object-name>ovhd_pnl_indicator</object-name>
    		<object-name>ovhd_flood_indicator</object-name>
    		<object-name>dashboard.text.indicator</object-name>
    		<object-name>dashboard.light.indicator</object-name>
    		<object-name>light.knobs</object-name>
		<emission>
			<red>1.5</red>
			<green>1.27</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/ovhd-panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<name>Panel lighting</name>
		<type>material</type>
		<object-name>ovhd_lcd</object-name>
		<emission>
			<red>1.39</red>
			<green>0</green>
			<blue>0</blue>
			<factor-prop>controls/lighting/ovhd-panel-norm</factor-prop>
		</emission>
	</animation>
	
    	<animation>
		<name>OVHD light knob</name>
		<type>pick</type>
		<object-name>ovhd_pnl_knob</object-name>
    		<object-name>ovhd_pnl_indicator</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/ovhd-panel-norm</property>
				<step>0.025</step>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/lighting/ovhd-panel-norm</property>
				<step>-0.025</step>
				<min>0</min>
			</binding>
		</action>
	</animation>
	
	<animation>
		<name>OVHD panel light rotate</name>
		<type>rotate</type>
		<object-name>ovhd_pnl_knob</object-name>
    		<object-name>ovhd_pnl_indicator</object-name>
		<property>controls/lighting/ovhd-panel-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>-329</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
