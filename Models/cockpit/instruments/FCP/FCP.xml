            <x-m>-25.7162</x-m>
            <y-m>-0.0924</y-m>
            <z-m>11.767</z-m>
 		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<name>MACH-knob</name>
		<type>rotate</type>
		<condition>
			<property>instrumentation/afds/inputs/ias-mach-selected</property>
		</condition>
		<object-name>SpdKnob</object-name>
			<property>autopilot/settings/target-speed-mach</property>
		<factor>10000</factor>
