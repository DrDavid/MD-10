<!-- VS/FPA -->
	<animation>
		<type>select</type>
		<object-name>vs.ind</object-name>
		<condition>
			<and>
				<property>systems/electrical/outputs/avionics</property>
				<not>
					<property>instrumentation/afds/inputs/vs-fpa-selected</property>
				</not>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>VS.led1</object-name>
		<object-name>VS.led2</object-name>
		<object-name>VS.led3</object-name>
		<object-name>VS.led4</object-name>
		<condition>
			<and>
				<property>systems/electrical/outputs/avionics</property>
				<equals>
					<property>instrumentation/afds/ap-modes/pitch-mode</property>
					<value>V/S</value>
				</equals>
				<not>
					<property>instrumentation/afds/inputs/vs-fpa-selected</property>
				</not>
			</and>
		</condition>
	</animation>
	
		<animation>
		<type>select</type>
		<object-name>alt_zero</object-name>
		<condition>
			<and>
				<property>systems/electrical/outputs/avionics</property>
					<not><equals><property>instrumentation/afds/ap-modes/pitch-mode</property>
					<value>V/S</value>
					</equals>
					</not>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>VS.ledminus</object-name>
		<condition>
			<and>
			   <not>
					<property>instrumentation/afds/inputs/vs-fpa-selected</property>
			   </not>
				<less-than>
					<property>autopilot/settings/vertical-speed-fpm</property>
					<value>0</value>
				</less-than>
				<equals>
					<property>instrumentation/afds/ap-modes/pitch-mode</property>
					<value>V/S</value>
				</equals>
			</and>
		</condition>
	</animation>

