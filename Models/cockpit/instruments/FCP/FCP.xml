<!-- LED displays-->

<!-- IAS/MACH -->
	<animation>
		<type>select</type>
		<object-name>ias.ind</object-name>
		<object-name>IAS.led1</object-name>
		<object-name>IAS.led2</object-name>
		<object-name>IAS.led3</object-name>
		<condition>
			<and>
				<property>systems/electrical/outputs/avionics</property>
				<not>
					<property>instrumentation/afds/inputs/ias-mach-selected</property>
				</not>
				<not-equals>
					<property>instrumentation/afds/ap-modes/pitch-mode</property>
					<value>VNAV ALT</value>
				</not-equals>
				<not-equals>
					<property>instrumentation/afds/ap-modes/pitch-mode</property>
					<value>VNAV SPD</value>
				</not-equals>
				<or>
					<not-equals>
						<property>instrumentation/afds/ap-modes/pitch-mode</property>
						<value>VNAV PTH</value>
					</not-equals>
					<and>
						<equals>
							<property>instrumentation/afds/ap-modes/pitch-mode</property>
							<value>VNAV PTH</value>
						</equals>
						<property>instrumentation/afds/settings/manual-intervention</property>
					</and>
				</or>
			</and>
		</condition>
	</animation>


	<animation>
		<type>textranslate</type>
