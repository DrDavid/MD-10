	<animation>
		<type>pick</type>
		<object-name>MACH.toggle</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
			  <condition>
				 <greater-than-equals>
					<property>instrumentation/airspeed-indicator/indicated-mach</property>
					<value>0.40</value>
				 </greater-than-equals>
			   </condition>
			   <command>property-toggle</command>
			   <property>instrumentation/afds/inputs/ias-mach-selected</property>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.click(4);</script>
			</binding>
		</action>
	</animation>

