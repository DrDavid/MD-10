	<animation>
		<name>Altitude setting 1's</name>
		<type>rotate</type>
		<object-name>alt.circle5</object-name>
				<property>controls/autoflight/altitude-select</property>
		<factor>36</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>SpdKnob</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/autoflight/autothrottle-engage[0]</property>
			</binding>
						<binding>
				<command>property-toggle</command>
				<property>controls/autoflight/autothrottle-engage[1]</property>
			</binding>
		</action>
		<action>
			<button>3</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
		<property>autopilot/settings/target-speed-kt</property>
				<step>2</step>
			</binding>
		</action>
				<action>
			<button>4</button>
			<repeatable type="bool">true</repeatable>
			<binding>
				<command>property-adjust</command>
		<property>autopilot/settings/target-speed-kt</property>
				<step>-2</step>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>auto.engage</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/autoflight/autopilot[0]/engage-mode</property>
				<step>2</step>
				<max>2</max>
				<min>-2</min>
			</binding>
		</action>
		</animation>
		
			<animation>
		<name>IAS setting select</name>
		<type>select</type>
		<object-name>IAS.led1</object-name>
		<object-name>IAS.led2</object-name>
		<object-name>IAS.led3</object-name>
		<condition>
			<equals>
				<property>controls/autoflight/autothrottle-engage[0]</property>
				<value>true</value>
			</equals>
		</condition>
	</animation>
	
				<animation>
		<name>IAS setting select</name>
		<type>select</type>
		<object-name>at_dis1</object-name>
		<condition>
			<equals>
				<property>controls/autoflight/autothrottle-engage[0]</property>
				<value>false</value>
			</equals>
		</condition>
	</animation>
	
		<!-- Disengage -->
	<animation>
		<type>pick</type>
		<object-name>auto.engage</object-name>
