<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>MCP.ac</path>
	
  
<!-- MCP -->

	<animation>
		<type>textranslate</type>
		<object-name>HDG.led1</object-name>
		<property>controls/autoflight/heading-select</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
