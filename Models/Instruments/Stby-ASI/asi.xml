<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>asi.ac</path>

	<params>
		<indicated-speed-kt>instrumentation/airspeed-indicator/indicated-speed-kt</indicated-speed-kt>
	</params>

	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.273</blue>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property alias="../../params/indicated-speed-kt" />
		<interpolation>
			<entry><ind>0</ind><dep>10</dep></entry>
			<entry><ind>60</ind><dep>0</dep></entry>
			<entry><ind>100</ind><dep>-67</dep></entry>
			<entry><ind>200</ind><dep>-180</dep></entry>
			<entry><ind>450</ind><dep>-330</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
