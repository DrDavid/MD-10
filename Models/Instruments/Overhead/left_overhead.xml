<PropertyList>

  <path>plaque.ac</path>

  <!-- Civilian red version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-red</value>
      </equals>
    </condition>
    <texture>colors.png</texture>
  </animation>

  <!-- Civilian gray version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-gray</value>
      </equals>
    </condition>
    <texture>colors_gray.png</texture>
  </animation>

  <!-- Military version -->
  <animation>
    <type>material</type>
    <object-name>plaque</object-name>
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>military</value>
      </equals>
    </condition>
    <texture>colors_military.png</texture>
  </animation>

