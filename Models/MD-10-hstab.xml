<?xml version="1.0" encoding="UTF-8" ?>

<!-- McDonnell Douglas MD-10 -->
<!-- Horizontal stabilizer model -->

<PropertyList>

	<path>MD-10-hstab.ac</path>

	<animation>
		<name>Left elevator</name>
		<type>rotate</type>
		<object-name>ElevatorL</object-name>
		<property>controls/flight/elevator</property>
		<factor>40</factor>
		<axis>
			<x1-m>25.969</x1-m>
			<y1-m>-10.736</y1-m>
			<z1-m>7.434</z1-m>
			<x2-m>21.62</x2-m>
			<y2-m>-1.358</y2-m>
			<z2-m>5.714</z2-m>
		</axis>
	</animation>
	<animation>
		<name>Right elevator</name>
		<type>rotate</type>
		<object-name>ElevatorR</object-name>
		<property>controls/flight/elevator</property>
		<factor>40</factor>
		<axis>
			<x1-m>21.62</x1-m>
			<y1-m>1.358</y1-m>
			<z1-m>5.714</z1-m>
			<x2-m>25.969</x2-m>
			<y2-m>10.736</y2-m>
			<z2-m>7.434</z2-m>
		</axis>
	</animation>
	<animation>
		<name>Elevator trim</name>
		<type>rotate</type>
		<object-name>Hstabs</object-name>
		<object-name>ElevatorL</object-name>
		<object-name>ElevatorR</object-name>
		<property>controls/flight/elevator-trim</property>
		<interpolation>
			<entry><ind>-1</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>10</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>20.97</x-m>
			<y-m>0</y-m>
			<z-m>5.465</z-m>
		</center>
	</animation>

</PropertyList>
