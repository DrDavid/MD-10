<!-- Speedbrake/spoiler lever -->
<animation>
<type>pick</type>
<object-name>SpeedbrakeLever</object-name>
<action>
<button>0</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/spoilers</property>
<step>0.2</step>
<max>1</max>
</binding>
</action>
<action>
<button>1</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/flight/spoilers</property>
<step>-0.2</step>
<min>0</min>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>SpeedbrakeLever</object-name>
<property>controls/flight/spoilers</property>
<interpolation>
<entry><ind>0</ind><dep>-33</dep></entry>
<entry><ind>0.2</ind><dep>2</dep></entry>
<entry><ind>0.4</ind><dep>10</dep></entry>
<entry><ind>0.6</ind><dep>18</dep></entry>
<entry><ind>0.8</ind><dep>30</dep></entry>
<entry><ind>1</ind><dep>40</dep></entry>
</interpolation>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>

<!-- Engine 1 thrust lever -->
<animation>
<type>pick</type>
<object-name>Eng1ThrustATOff</object-name>
<action>
<button>0</button>
<repeatable type="bool">false</repeatable>
<binding>
<command>property-assign</command>
<property>controls/autoflight/autothrottle-engage[0]</property>
<value type="bool">false</value>
</binding>
</action>
</animation>
<animation>
<type>pick</type>
<object-name>Eng1ThrustLever</object-name>
<action>
<button>0</button>
<button>3</button>
<repeatable type="bool">true</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/engines/engine[0]/throttle</property>
<step>0.01</step>
<max>1</max>
</binding>
</action>
<action>
<button>1</button>
<button>4</button>
<repeatable type="bool">true</repeatable>
<binding>
<command>property-adjust</command>
<property>controls/engines/engine[0]/throttle</property>
<step>-0.01</step>
<min>0</min>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>Eng1ThrustLever</object-name>
<object-name>Eng1ThrustATOff</object-name>
<object-name>Eng1Reverser</object-name>
<property>controls/engines/engine[0]/throttle</property>
<interpolation>
<entry><ind>0</ind><dep>8</dep></entry>
<entry><ind>0.6</ind><dep>-34</dep></entry>
<entry><ind>1</ind><dep>-43</dep></entry>
</interpolation>
<condition>
<not>
<property>controls/engines/engine[0]/reverser</property>
</not>
</condition>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.548</x-m>
<y-m>0</y-m>
<z-m>0.522</z-m>
</center>
</animation>
<animation>
<type>pick</type>
<object-name>Eng1Reverser</object-name>
<action>
<button>0</button>
<binding>
<command>nasal</command>
<script>
MD10.engine1.reverse_thrust();
</script>
</binding>
</action>
</animation>
<animation>
<type>rotate</type>
<object-name>Eng1Reverser</object-name>
<property>controls/engines/engine[0]/throttle</property>
<factor>140</factor>
<condition>
<property>controls/engines/engine[0]/reverser</property>
</condition>
<axis>
<x>0</x>
<y>1</y>
<z>0</z>
</axis>
<center>
<x-m>-2.531</x-m>
<y-m>0</y-m>
<z-m>0.87</z-m>
</center>
</animation>
