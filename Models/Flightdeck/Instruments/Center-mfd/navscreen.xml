<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

	<path>center-mfd.ac</path>

	<animation>
		<!-- Opaque objects -->
		<type>material</type>
		<object-name>cadre</object-name>    
		<object-name>vis</object-name>    
		<object-name>trim1</object-name>
		<object-name>trim2</object-name>
		<object-name>trim3</object-name>
		<object-name>bt-b1</object-name>
		<object-name>bt-b2</object-name>
		<object-name>bt-b3</object-name>
		<object-name>bt-b4</object-name>
		<object-name>bt-h1</object-name>
		<object-name>bt-h2</object-name>
		<object-name>bt-h3</object-name>
		<object-name>bt-h4</object-name>
		<object-name>bt-h5</object-name>
		<object-name>pot-l1</object-name>
		<object-name>pot-l2</object-name>
		<object-name>pot-l3</object-name>
		<object-name>pot-l4</object-name>
		<object-name>pot-r1</object-name>
		<object-name>pot-r2</object-name>
		<object-name>pot-r3</object-name>
		<object-name>pot-r4</object-name>
		<object-name>blackbkd</object-name>
	</animation>

	<animation>
		<type>material</type>
		<object-name>cadre</object-name>    
		<object-name>vis</object-name>    
		<object-name>trim1</object-name>
		<object-name>trim2</object-name>
		<object-name>trim3</object-name>
		<object-name>bt-b1</object-name>
		<object-name>bt-b2</object-name>
		<object-name>bt-b3</object-name>
		<object-name>bt-b4</object-name>
		<object-name>bt-h1</object-name>
		<object-name>bt-h2</object-name>
		<object-name>bt-h3</object-name>
		<object-name>bt-h4</object-name>
		<object-name>bt-h5</object-name>
		<object-name>pot-l1</object-name>
		<object-name>pot-l2</object-name>
		<object-name>pot-l3</object-name>
		<object-name>pot-l4</object-name>
		<object-name>pot-r1</object-name>
		<object-name>pot-r2</object-name>
		<object-name>pot-r3</object-name>
		<object-name>pot-r4</object-name>
		<object-name>blackbkd</object-name>
		<emission>      
			<factor> 0.13 </factor>
			<red>   1 </red>
			<green> 1 </green>
			<blue>  1 </blue>
		</emission>
	</animation>
  
	<animation>
		<type>pick</type>
		<object-name>blackbkd</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>MD10.showNd();</script>
			</binding>
		</action>
	</animation>
  
	<!--######################## Change display mode  ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-b1</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-cycle</command>
				<property>/instrumentation/efis/mfd/display-mode</property>
				<value>APP</value>
				<value>MAP</value>
				<value>PLAN</value>
				<value>VOR</value>
			</binding>
		</action>
	</animation>

	<!--######################## Change display type  ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-b2</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-cycle</command>
				<property>/instrumentation/efis/mfd/display-type</property>
          			<!--<value>CRT</value> -->
				<value>LCD</value>
			</binding>
		</action>
	</animation>

	<!--######################## Center ND modes   ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-b3</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/nd-centered</property>
			</binding>
		</action>
	</animation>
  
	<!--######################## Way Point Data ########################-->
	<animation>
		<type>pick</type>
		<object-name>bt-b4</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/data</property>
			</binding>
		</action>
	</animation>

	<!--######################## Airports ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-h1</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/arpt</property>
			</binding>
		</action>
	</animation>

	<!--######################## Traffic ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-h2</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/tfc</property>
				<!-- This is anyway a bool, so toggle is only true/false -->
				<!-- value>true</value>
				<value>false</value -->
			</binding>
		</action>
	</animation>

	<!--######################## Wpt ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-h3</object-name>
		<visible>true</visible>
		<action>
        		<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/wpt</property>
			</binding>
		</action>
	</animation>

	<!--######################## Position point ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-h4</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/pos</property>
			</binding>
		</action>
	</animation>

	<!--######################## Weather ############################-->
	<animation>
		<type>pick</type>
		<object-name>bt-h5</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>/instrumentation/efis/inputs/wxr</property>
			</binding>
		</action>
	</animation>

	<!-- ###################### Distance #############################-->
	<animation>
		<type>pick</type>
		<object-name>trim1</object-name>
		<action>
			<button>4</button> <!--  scroll up -->
			<repeatable>false</repeatable>   
			<binding>
				<command>property-cycle</command>
				<property>/instrumentation/efis/inputs/range-nm</property>
				<value>10</value>
				<value>20</value>
				<value>40</value>
				<value>80</value>
				<value>160</value>
				<value>320</value>
			</binding>   
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>trim1</object-name>
		<action>
			<button>3</button> <!--  scroll dn -->
			<repeatable>false</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>/instrumentation/efis/inputs/range-nm</property>
				<value>320</value>
				<value>160</value>
				<value>80</value>
				<value>40</value>
				<value>20</value>
				<value>10</value>              
			</binding>
		</action>
	</animation>

</PropertyList>
